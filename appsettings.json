{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Information",
      "Microsoft.AspNetCore.Authentication.JwtBearer": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "Development": {
    "ClientURL": "ASPNETCORE_CLIENT_URL"
  },
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "/etc/pki/tls/certs/tealpromethium/domain.cert.pem",
        "KeyPath": "/etc/pki/tls/certs/tealpromethium/private.key.pem"
      }
    }
  }
}
